{
  "name": "jupyterlab-rtc-mcp",
  "version": "0.1.0",
  "description": "MCP server for JupyterLab real-time collaboration",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "jupyterlab-rtc-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "start": "node dist/index.js",
    "start:sse": "node dist/index.js -sse -ip 127.0.0.1 -port 3000",
    "start:sse:debug": "node dist/index.js -sse -ip 127.0.0.1 -port 3000 -log debug",
    "start:sse:custom": "node dist/index.js -sse"
  },
  "dependencies": {
    "@jupyterlab/coreutils": "^6.4.5",
    "@jupyterlab/services": "^7.4.5",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "express": "^5.1.0",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.27",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^22.17.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "jupyterlab",
    "mcp",
    "ai",
    "collaboration",
    "real-time"
  ],
  "author": "Jupyter Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/complyue/jupyterlab-rtc-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/complyue/jupyterlab-rtc-mcp/issues"
  },
  "homepage": "https://github.com/complyue/jupyterlab-rtc-mcp#readme"
}
